# Production environment variables
# Copy to .env and fill in values

# Server
PORT=3001
NODE_ENV=production

# Google Cloud Storage (production - no GCS_ENDPOINT = uses real GCS)
GCS_BUCKET=pm-market-images
GCS_PROJECT_ID=your-gcp-project

# Blockchain - Mantle Sepolia Testnet
RPC_URL=https://rpc.sepolia.mantle.xyz
CHAIN_ID=5003

# Contract Addresses (deploy contracts first)
MARKET_FACTORY_ADDRESS=0x...
CONDITIONAL_TOKENS_ADDRESS=0x...
NEG_RISK_ADAPTER_ADDRESS=0x...

# Admin wallet for creating markets on-chain
ADMIN_PRIVATE_KEY=0x...
